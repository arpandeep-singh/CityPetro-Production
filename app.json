{
  "expo": {
    "name": "CityPetro",
    "slug": "CityPetro",
    "version": "1.0.6",
    "platforms": ["ios", "android", "web"],
    "orientation": "portrait",
    "icon": "./assets/cpIcon.png",
    "splash": {
      "image": "./assets/cpSplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#3366FF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": false
    },
    "android": {
      "package": "com.citypetro.transport",
      "versionCode": 7,
      "adaptiveIcon": {
        "foregroundImage": "./assets/cpAdapIcon.png",
        "backgroundColor": "#3366FF"
      }
    },
    "web": {
      "favicon": "./assets/cpFavIcon.png"
    },
    "description": "City Petro App",
    "notification": {
      "iosDisplayInForeground": true
    }
  }
}
